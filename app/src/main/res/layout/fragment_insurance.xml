<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/clInsuranceOut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/bg_primary">

        <androidx.core.widget.NestedScrollView
            android:id="@+id/nsvInsurance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clInsurance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg_primary">

                <View
                    android:id="@+id/viewInsuranceMg1"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="20:35"
                    app:layout_constraintWidth_percent="0.0556"
                    app:layout_constraintStart_toStartOf="parent"/>

                <View
                    android:id="@+id/viewInsuranceMg2"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="20:35"
                    app:layout_constraintWidth_percent="0.0556"
                    app:layout_constraintEnd_toEndOf="parent"/>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cvInsuranceMypetBoxBg"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="10dp"
                    app:layout_constraintDimensionRatio="320:212"
                    app:layout_constraintWidth_percent="0.8889"
                    app:layout_constraintStart_toEndOf="@id/viewInsuranceMg1"
                    app:layout_constraintEnd_toStartOf="@id/viewInsuranceMg2"
                    app:layout_constraintTop_toBottomOf="@id/viewInsuranceMg1"/>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:elevation="3dp"
                    app:layout_constraintStart_toStartOf="@id/cvInsuranceMypetBoxBg"
                    app:layout_constraintEnd_toEndOf="@id/cvInsuranceMypetBoxBg"
                    app:layout_constraintTop_toTopOf="@id/cvInsuranceMypetBoxBg"
                    app:layout_constraintBottom_toBottomOf="@id/cvInsuranceMypetBoxBg">

                    <View
                        android:id="@+id/viewInsuranceBoxMg1"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintDimensionRatio="20:20"
                        app:layout_constraintWidth_percent="0.0625"
                        app:layout_constraintStart_toStartOf="parent"/>

                    <View
                        android:id="@+id/viewInsuranceBoxMg2"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintDimensionRatio="20:20"
                        app:layout_constraintWidth_percent="0.0625"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>

                    <ImageView
                        android:id="@+id/ivInsuranceBoxPlus"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintDimensionRatio="32:32"
                        app:layout_constraintWidth_percent="0.1"
                        android:background="@drawable/ic_circle_add_mypet"
                        app:layout_constraintStart_toEndOf="@id/viewInsuranceBoxMg1"
                        app:layout_constraintTop_toBottomOf="@id/viewInsuranceBoxMg1"/>

                    <ImageView
                        android:id="@+id/ivInsuranceBoxTriangle"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_marginStart="8dp"
                        app:layout_constraintDimensionRatio="17:14"
                        app:layout_constraintWidth_percent="0.045"
                        android:background="@drawable/ic_triangle_mypet"
                        app:layout_constraintStart_toEndOf="@id/ivInsuranceBoxPlus"
                        app:layout_constraintTop_toTopOf="@id/ivInsuranceBoxPlus"
                        app:layout_constraintBottom_toBottomOf="@id/ivInsuranceBoxPlus"/>


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clInsuranceBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="@id/ivInsuranceBoxPlus"
                        app:layout_constraintTop_toBottomOf="@id/ivInsuranceBoxPlus"
                        app:layout_constraintEnd_toEndOf="@id/btnGoToMypet"
                        app:layout_constraintBottom_toTopOf="@id/btnGoToMypet">

                        <TextView
                            android:id="@+id/tvInsuranceBox1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="@style/b3_S"
                            android:text="@string/insurance_no_register_box1"
                            />

                        <TextView
                            android:id="@+id/tvInsuranceBox2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="@style/insurance_box_light"
                            android:textColor="@color/neutral_400"
                            android:text="@string/insurance_no_register_box2"
                            android:layout_marginTop="4dp"
                            app:layout_constraintTop_toBottomOf="@id/tvInsuranceBox1"
                            />

                    </androidx.constraintlayout.widget.ConstraintLayout>


                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btnGoToMypet"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="@string/go_to_mypet"
                        android:textAppearance="@style/b2_S"
                        android:textColor="@color/white"
                        android:stateListAnimator="@null"
                        android:background="@drawable/bg_rectangle_filled_point_blue_radius_8"
                        app:layout_constraintStart_toEndOf="@id/viewInsuranceBoxMg1"
                        app:layout_constraintEnd_toStartOf="@id/viewInsuranceBoxMg2"
                        app:layout_constraintBottom_toTopOf="@id/viewInsuranceBoxMg2"/>
                </androidx.constraintlayout.widget.ConstraintLayout>

                <View
                    android:id="@+id/viewInsuranceInfoBoxMg1"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="20:20"
                    app:layout_constraintWidth_percent="0.0556"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/cvInsuranceMypetBoxBg"/>

                <View
                    android:id="@+id/viewInsuranceInfoBoxMg2"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="20:20"
                    app:layout_constraintWidth_percent="0.0556"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/cvInsuranceMypetBoxBg"/>


                <androidx.cardview.widget.CardView
                    android:id="@+id/cvInsuranceInfoBoxBg"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="10dp"
                    app:layout_constraintDimensionRatio="320:442"
                    app:layout_constraintWidth_percent="0.8889"
                    app:layout_constraintStart_toEndOf="@id/viewInsuranceInfoBoxMg1"
                    app:layout_constraintEnd_toStartOf="@id/viewInsuranceInfoBoxMg2"
                    app:layout_constraintTop_toBottomOf="@id/viewInsuranceInfoBoxMg1"/>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clInsuranceInfoBox"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:elevation="3dp"
                    app:layout_constraintStart_toStartOf="@id/cvInsuranceInfoBoxBg"
                    app:layout_constraintEnd_toEndOf="@id/cvInsuranceInfoBoxBg"
                    app:layout_constraintTop_toTopOf="@id/cvInsuranceInfoBoxBg"
                    app:layout_constraintBottom_toBottomOf="@id/cvInsuranceInfoBoxBg">

                    <View
                        android:id="@+id/viewInsuranceInfoBoxTopMg1"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintDimensionRatio="24:32"
                        app:layout_constraintWidth_percent="0.075"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <View
                        android:id="@+id/viewInsuranceInfoBoxTopMg2"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintDimensionRatio="24:32"
                        app:layout_constraintWidth_percent="0.075"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>

                    <TextView
                        android:id="@+id/tvInsuranceInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:textAppearance="@style/insurance_box_bold"
                        android:textColor="@color/black"
                        android:text="@string/insurance_no_register_info_box1"
                        app:layout_constraintTop_toBottomOf="@id/viewInsuranceInfoBoxTopMg1"
                        app:layout_constraintStart_toEndOf="@id/viewInsuranceInfoBoxTopMg1"
                        app:layout_constraintEnd_toStartOf="@id/viewInsuranceInfoBoxTopMg2"
                        />

                    <ImageView
                        android:id="@+id/ivInsuranceInfoBoxMain"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:src="@drawable/bg_insurance"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="28dp"
                        app:layout_constraintStart_toStartOf="@id/tvInsuranceInfo"
                        app:layout_constraintEnd_toEndOf="@id/tvInsuranceInfo"
                        app:layout_constraintTop_toBottomOf="@id/tvInsuranceInfo"
                        app:layout_constraintBottom_toTopOf="@id/tvInsuranceBottomBoxInfo"
                        app:layout_constraintDimensionRatio="272:250"
                        />

                    <TextView
                        android:id="@+id/tvInsuranceBottomBoxInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:textAppearance="@style/b5_S"
                        android:textColor="@color/text_disable"
                        android:text="@string/insurance_no_register_info_box2"
                        app:layout_constraintBottom_toTopOf="@id/viewInsuranceInfoBoxBottomMg1"
                        app:layout_constraintStart_toEndOf="@id/viewInsuranceInfoBoxTopMg1"
                        app:layout_constraintEnd_toStartOf="@id/viewInsuranceInfoBoxTopMg2"
                        />

                    <View
                        android:id="@+id/viewInsuranceInfoBoxBottomMg1"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintDimensionRatio="24:40"
                        app:layout_constraintWidth_percent="0.075"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>
                </androidx.constraintlayout.widget.ConstraintLayout>

                <View
                    android:id="@+id/viewInsuranceBottomMg1"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="20:64"
                    app:layout_constraintWidth_percent="0.0556"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/cvInsuranceInfoBoxBg"/>

                <View
                    android:id="@+id/viewInsuranceBottomMg2"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="20:48"
                    app:layout_constraintWidth_percent="0.0556"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tvInsuranceBottomInfo"/>


                <TextView
                    android:id="@+id/tvInsuranceBottomInfo"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="@style/describe_b5_M"
                    android:textColor="@color/neutral_500"
                    android:text="@string/insurance_no_register_bottom"
                    app:layout_constraintTop_toBottomOf="@id/viewInsuranceBottomMg1"
                    app:layout_constraintStart_toEndOf="@id/viewInsuranceBottomMg1"
                    app:layout_constraintEnd_toStartOf="@id/viewInsuranceBottomMg2"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>